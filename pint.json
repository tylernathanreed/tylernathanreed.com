{
    "preset": "psr12",
    "rules": {
        "new_with_braces": false,
        "not_operator_with_successor_space": true,
        "ordered_imports": true,
        "phpdoc_scalar": true,
        "phpdoc_separation": true,
        "single_space_around_construct": true,
        "single_trait_insert_per_statement": false
    }
}
